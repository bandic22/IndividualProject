    <!--Bootstrap stuff-->
    <script src="~/Scripts/jquery-2.2.0.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <!--Angular stuff-->
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular-route.min.js"></script>
    <script src="~/Scripts/angular-resource.min.js"></script>
    <script src="~/Scripts/angular-ui/ui-bootstrap-tpls.min.js"></script>
    <script src="~/Scripts/angular-ui-router.js"></script>
    <script src="~/Scripts/angular-sanitize.min.js"></script>
    <script type="text/javascript" src="//api.filestackapi.com/filestack.js"></script>
    <script src="/Scripts/angular_filepicker.min.js"></script>
    <script src="~/Scripts/moment.js"></script>
    <!--Files created by me-->
    <script src="~/ngApp/ngApp.js"></script>
    <script src="~/audiojs/audio.min.js"></script>
    <script src="~/ngApp/Services/exploreServices.js"></script>
    <script src="~/ngApp/Services/userServices.js"></script>
    <script src="~/ngApp/Services/accountService.js"></script>
    <script src="~/ngApp/Services/profileService.js"></script>
    <script src="~/ngApp/Services/adminService.js"></script>
    <script src="~/ngApp/Services/categoryService.js"></script>
    <script src="~/ngApp/Controllers/aboutController.js"></script>
    <script src="~/ngApp/Controllers/exploreController.js"></script>
    <script src="~/ngApp/Controllers/homeController.js"></script>
    <script src="~/ngApp/Controllers/profileController.js"></script>
    <script src="~/ngApp/Controllers/accountController.js"></script>
    <script src="~/ngApp/Controllers/requestDetailsController.js"></script>
    <script src="~/ngApp/Controllers/adminController.js"></script>
    <script src="~/ngApp/Controllers/ModalControllers/editProfileModalController.js"></script>
    <script src="~/ngApp/Controllers/ModalControllers/editRequestController.js"></script>
    <script src="~/ngApp/Controllers/ModalControllers/editSpaceController.js"></script>
    <script src="~/ngApp/Controllers/ModalControllers/editGearController.js"></script>
    <script src="~/ngApp/Controllers/ModalControllers/gearDialogController.js"></script>
    <script src="~/ngApp/Controllers/ModalControllers/reqDialogController.js"></script>
    <script src="~/ngApp/Controllers/ModalControllers/spaceDialogController.js"></script>
    <script src="~/ngApp/Controllers/spaceDetailsController.js"></script>
    <script src="~/ngApp/Controllers/gearDetailsController.js"></script>
    <script src="~/ngApp/Controllers/ModalControllers/deleteModalController.js"></script>
    <script src="~/ngApp/Controllers/ModalControllers/imageModalController.js"></script>
</body>
</html>
